	<parameter name="student_id" class="java.lang.String"/>
	<query language="SQL"><![CDATA[select
	a.id,
	a.present,
	a.created_at,
	a.lesson_id,
	l.lesson_number,
	l.lesson_date,
	t.id as teacher_id,
	p.name as teacher_name
from app.attendances a
left join app.lessons l on l.id = a.lesson_id
left join app.teachings teaching on teaching.lesson_id = a.lesson_id
left join app.teachers t on t.id = teaching.teacher_id
left join app.people p on p.id = t.person_id
where a.student_id =  CAST($P{student_id} AS UUID) 
order by l.lesson_number asc
]]></query>
